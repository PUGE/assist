{
	"manifest_version": 2,
	"name": "脚本助手",
	"version": "1.0.5",
	"description": "脚本助手",
	"icons": {
		"16": "./img/16.png",
		"48": "./img/48.png",
		"128": "./img/128.png"
	},
	"browser_action":  {
		"default_icon": "img/48.png",
		"default_title": "这是一个示例Chrome插件",
		"default_popup": "popup.html"
	},
	"content_scripts": [],
	"permissions": [
		"activeTab",
		"notifications",
		"webRequest",
		"https://going.run/*"
	],
	"homepage_url": "https://github.com/PUGE",
	"options_page": "options.html",
	"options_ui":
	{
		"page": "options.html",
		"chrome_style": true
	}
}